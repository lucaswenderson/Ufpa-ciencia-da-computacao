Matriz = [[0]*12,[0]*12,[0]*12,[0]*12,[0]*12,[0]*12,[0]*12,[0]*12,[0]*12,[0]*12,[0]*12,[0]*12]
soma = 0
for i in range(12):
  for j in range(12):
    Matriz[i][j] = int(input("Insira o valor para Matriz[%i][%i]: " %(i,j)))
    if(i + j > 11):
      soma += Matriz[i][j]
print("\nSoma dos valores >", soma)