def trianguloPascal(n):
  matriz = [[1],[1,1]]
  for i in range(1,n):
      linha = [1]
      for j in range(0,len(matriz[i])-1):
          linha += [ matriz[i][j] + matriz[i][j+1] ]
      linha += [1]
      matriz += [linha]
  return matriz

n = int(input("Digite o n√∫mero de linhas: "))
resultado = trianguloPascal(n)

for i in range(len(resultado)-1):
    print(resultado[i])