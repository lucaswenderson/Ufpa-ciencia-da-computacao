if(age >= 65); //erro ';'
  System.out.println("Age is greater then or equal to 65");
else
  System.out.println("Age is less than 65)"; //erro ' )" '


int x = 1, total;
while( x <= 10)
{
  total += x; //erro 'total não foi inicializado'
  ++x;
}


while(x<=100) //erro 'while em loop infinito pois ++x não esta dentro do escopo'
  total += x;
  ++x;

while(y>0) //erro 'escopo do while não foi fechado, falta um }'
{
  System.out.println(y);
  ++y;